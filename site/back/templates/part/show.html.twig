{% extends 'base.html.twig' %}

{% block title %}Part{% endblock %}

{% block body %}
    <h1>Part</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ part.id }}</td>
            </tr>
            <tr>
                <th>Content</th>
                <td>{{ part.content }}</td>
            </tr>
            <tr>
                <th>Title</th>
                <td>{{ part.title }}</td>
            </tr>
            <tr>
                <th>Created_at</th>
                <td>{{ part.createdAt ? part.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Updated_at</th>
                <td>{{ part.updatedAt ? part.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_part_index') }}">back to list</a>

    <a href="{{ path('app_part_edit', {'id': part.id}) }}">edit</a>

    {{ include('part/_delete_form.html.twig') }}
{% endblock %}
